source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '11.4'

target 'MoonDime-Alarm' do
  # Comment the next line if you don't want to use dynamic frameworks
    use_frameworks!
    pod 'web3swift'
    pod 'FirebaseAnalytics'
    pod 'FirebaseAuth'
    pod 'FirebaseFirestore'

  # Pods for LocationAlarm

  target 'MoonDime-AlarmTests' do
    inherit! :search_paths
    use_frameworks!
    pod 'web3swift'
    pod 'FirebaseAuth'
    pod 'FirebaseFirestore'
  end

  target 'MoonDime-AlarmUITests' do
    use_frameworks!
    pod 'web3swift'
    pod 'FirebaseAuth'
    pod 'FirebaseFirestore'
  end

end
